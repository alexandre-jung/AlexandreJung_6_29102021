{
  "name": "fisheye",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev:sass": "sass --watch ./scss/main.scss ./css/main.css",
    "dev:server": "live-server --host=localhost --port=80 --entry-file=./index.html",
    "components:server": "live-server --host=localhost --port=8000 --mount=/:./components/ --ignore=node_modules,api,assets,scss,media,.gitignore,*.json .",
    "start": "concurrently -n dev: \"npm:dev:*\" --kill-others",
    "components": "concurrently -n sass,server \"npm:dev:sass\" \"npm:components:server\" --kill-others",
    "lint": "eslint \"js/**\""
  },
